{
  "name": "@ladesa-ro/api-client-fetch.docs",
  "version": "1.0.0-next.29",
  "files": ["dist/**/*.*"],
  "private": true,
  "scripts": {
    "generate": "nx generate",
    "generate:watch": "typedoc --options .config/typedoc.jsonc --watch --preserveWatchOutput",
    "build": "nx build",
    "serve": "nx serve"
  },
  "nx": {
    "tags": ["integrations", "integrations:npm"],
    "root": "integrations/npm/api-client-fetch-docs",
    "namedInputs": {
      "default": ["{projectRoot}/.config/**/*"]
    },
    "targets": {
      "serve": {
        "cache": false,
        "executor": "nx:run-commands",
        "options": {
          "cwd": "{projectRoot}",
          "command": "serve dist"
        },
        "dependsOn": ["build"]
      },
      "build": {
        "cache": false,
        "executor": "nx:run-commands",
        "inputs": [],
        "options": {
          "cwd": "{projectRoot}",
          "command": "echo noop"
        },
        "outputs": [],
        "dependsOn": ["generate"]
      },
      "generate": {
        "cache": true,
        "executor": "nx:run-commands",
        "inputs": ["default", "^default"],
        "options": {
          "cwd": "{projectRoot}",
          "command": "typedoc --options .config/typedoc.jsonc"
        },
        "outputs": [],
        "dependsOn": ["@ladesa-ro/api-client-fetch:build"]
      }
    }
  },
  "devDependencies": {
    "serve": "^14.2.4",
    "typedoc": "^0.27.6",
    "typedoc-plugin-mdn-links": "^4.0.8"
  }
}
